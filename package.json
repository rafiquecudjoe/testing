{
  "name": "kitkat-serverless",
  "version": "1.0.0",
  "description": "Serverless webpack example using Typescript",
  "main": "handler.js",
  "scripts": {
    "preinstall": "npx only-allow yarn",
    "dev": "NODE_ENV=development yarn start",
    "start": "tsnd --files src/index.ts",
    "migrate:init": "npx prisma migrate dev --name init",
    "test": "jest --watch --runInBand",
    "ci": "tsc --noEmit && jest --runInBand --coverage --ci --coverageDirectory ./coverage/",
    "fix:lint": "eslint src --fix",
    "fix:style": "prettier src --write",
    "check:lint": "eslint src",
    "check:style": "prettier src --check",
    "swagger-ui": "run-p swagger-ui:serve swagger-ui:open",
    "swagger-ui:serve": "serve -p 5000 ./",
    "swagger-ui:open": "open http://localhost:5000/doc/swagger-ui.html"
  },
  "dependencies": {
    "@google-cloud/language": "^4.2.2",
    "@prisma/client": "^3.3.0",
    "@slack/bolt": "^3.8.1",
    "@slack/oauth": "^2.2.0",
    "@slack/web-api": "^5.10.0",
    "@types/jest": "^26.0.14",
    "@types/sequelize": "^4.28.9",
    "cors": "^2.8.5",
    "dayjs": "^1.9.3",
    "dotenv": "^10.0.0",
    "express-openapi-validator": "^4.13.2",
    "jest-mock-extended": "^1.0.18",
    "jsonwebtoken": "^8.5.1",
    "mysql": "^2.18.1",
    "mysql2": "^2.1.0",
    "node-fetch": "^2.6.1",
    "source-map-support": "^0.5.10",
    "sqlite3": "^5.0.0",
    "ts-jest": "^26.4.1"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.10.4",
    "@slack-wrench/jest-mock-web-client": "^1.3.0",
    "@types/aws-lambda": "^8.10.59",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jest-image-snapshot": "^4.1.3",
    "@types/node": "14",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^4.29.2",
    "@typescript-eslint/parser": "^4.29.2",
    "aws-lambda-mock-context": "^3.2.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "express": "^4.17.1",
    "fork-ts-checker-webpack-plugin": "^3.0.1",
    "jest": "^26.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.3.2",
    "prisma": "^2.30.2",
    "sequelize-cli": "^6.2.0",
    "serve": "^12.0.0",
    "sinon": "^11.1.2",
    "supertest": "^6.1.6",
    "ts-loader": "^5.3.3",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.4.2"
  },
  "author": "The serverless webpack authors (https://github.com/elastic-coders/serverless-webpack)",
  "license": "MIT"
}
